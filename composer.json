{
    "name": "devsohail/warehouse-framework",
    "description": "Laravel Warehouse Framework",
    "keywords": [
        "wms",
        "laravel",
        "warehouse",
        "warehouse management system"
    ],
    "homepage": "https://github.com/devsohail/warehouse-framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Sohail Sajid",
            "email": "sohailsajid32@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "illuminate/console": "^9.0 || ^10.0",
        "illuminate/contracts": "^9.0 || ^10.0",
        "illuminate/database": "^9.0 || ^10.0",
        "illuminate/queue": "^9.0 || ^10.0",
        "illuminate/support": "^9.0 || ^10.0",
        "devsohail/gtin": "^2.10",
        "devsohail/laravel-model-expires": "^1.10",
        "spatie/laravel-model-states": "^2.2",
        "staudenmeir/eloquent-has-many-deep": "^1.15"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.0 || ^7.0",
        "orchestra/testbench": "^6.24 || ^7.0",
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Just\\Warehouse\\WarehouseServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Just\\Warehouse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Just\\Warehouse\\Tests\\": "tests"
        },
        "classmap": [
            "database/model-factories"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/devsohail/gtin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/devsohail/laravel-model-expires"
        },
        {
            "type": "vcs",
            "url": "https://github.com/devsohail/warehouse-framework"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
